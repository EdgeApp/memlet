    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "mocha": "^8.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.5",
    "rollup-plugin-filesize": "^9.0.2",
    "sucrase": "^3.15.0",
